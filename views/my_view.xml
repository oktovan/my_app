<?xml version="1.0" encoding="utf-8"?>

<odoo>
	<record id="my_tree_view" model="ir.ui.view">
		<field name="name">my.tree</field>
		<field name="model">my.model</field>
		<field name="arch" type="xml">
			<tree string="My Apps">
				<field name = "field1" />
				
			</tree>
		</field>
	</record>

	<record id="my_form_view" model="ir.ui.view">
		<field name = "name">my.form</field>
		<field name = "model">my.model</field>
		<field name = "arch" type="xml">
			<form string="My Apps">
				<sheet>
					<group>
						<group>
						<field name="field1"/>
						</group>
					</group>
				</sheet>
			</form>
		</field>
	</record>

	<record id="action_my_app" model="ir.actions.act_window">
		<field name="name">My App</field>
		<field name="type">ir.actions.act_window</field>
		<field name="res_model">my.model</field>
		<field name="view_mode">tree,form</field>
		<field name="context">{}</field>
		<field name="domain">[]</field>
		<field name="help" type="html">
			<p class="o_view_nocontent_smiling_face">
				Create Your Data Here!
			</p>
		</field>
	</record>

	<menuitem	id="my_app_root_menu"
				name="My App"
				web_icon="my_app,static/description/icon.png"
				sequence="7"/>

	<menuitem	id="my_app_menu"
				name="My App" action="action_my_app"
				parent="my_app_root_menu"
				sequence="10"/>
</odoo>
